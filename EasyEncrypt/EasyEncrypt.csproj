<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.3.2</Version>
    <Authors>Henkje</Authors>
    <Company>HenkSharp</Company>
    <Description>Library that makes encrypting strings, byte arrays and files easy. Supports Aes, TripleDES, (.net framework only, Des, RC2 and Rijndael)</Description>
    <Copyright>Copyright (C) 2019 henkje</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/GHenkje/EasyEncrypt</PackageProjectUrl>
    <RepositoryUrl>https://github.com/GHenkje/EasyEncrypt</RepositoryUrl>
    <PackageTags>Encryption, FileEncryption, Cryptography, Securety, Protection, HenkEncrypt, Aes, TripleDES, Des, RC2, Rijndael</PackageTags>
    <PackageReleaseNotes>- Added GetKey()
Which returns the current encryption key.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>EasyEncrypt.snk</AssemblyOriginatorKeyFile>
    <AssemblyVersion>1.0.3.2</AssemblyVersion>
    <FileVersion>1.0.3.2</FileVersion>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\SimpleHashing\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
