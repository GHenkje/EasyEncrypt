<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.3</Version>
    <Authors>Henkje</Authors>
    <Company>HenkSharp</Company>
    <Description>Library that makes encrypting strings, byte arrays and files easy. Supports Aes, TripleDES, (.net framework only) Des, RC2 and Rijndael. EasyEncrypt has a good documentation.</Description>
    <Copyright>Copyright (C) 2019 henkje</Copyright>
    <PackageLicenseUrl>https://github.com/GHenkje/EasyEncrypt/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/GHenkje/EasyEncrypt</PackageProjectUrl>
    <RepositoryUrl>https://github.com/GHenkje/EasyEncrypt</RepositoryUrl>
    <PackageTags>Encryption, FileEncryption, Cryptography, Securety, Protection, HenkEncrypt, Aes, TripleDES, Des, RC2, Rijndael</PackageTags>
    <PackageReleaseNotes>- Added GetKey()
Wich returns the current encryption key.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>EasyEncrypt.snk</AssemblyOriginatorKeyFile>
    <AssemblyVersion>1.0.3.0</AssemblyVersion>
    <FileVersion>1.0.3.0</FileVersion>
  </PropertyGroup>

</Project>
